<html>
  <body>
    <div>
      <script>
	const getJSON = async url => {
	    const response = await fetch(url);
	    if (!response.ok)
		throw new Error(response.statusText);
	    const data = response.json();
	    return data
	}

	console.log("Fetching data...")
	getJSON("https://codermerlni.com/vapor/david-ben-yaakov/employees").then(data =>{
	    console.log(data);
	}).catch(error => {
	    console.error(error);
	});	
	console.log("done")
	
      </script>
    </div>
  </body>
</html>    
